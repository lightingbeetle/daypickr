{
  "name": "daypickr",
  "version": "0.3.4",
  "description": "An accessible, interactive widget for selecting a day which populates an input field with a date.",
  "main": "public/daypickr.js",
  "files": [
    "public/*",
    "public/esm",
    "public/esm/*"
  ],
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node scripts/serve.js -w",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lightingbeetle/daypickr.git"
  },
  "keywords": [
    "datepicker"
  ],
  "author": "Juraj Kiss",
  "contributors": [
    "Dušan Tužinský (https://github.com/DushanT)",
    "Viktor Gaško (https://github.com/ViktorGasko)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lightingbeetle/daypickr/issues"
  },
  "homepage": "https://github.com/lightingbeetle/daypickr#readme",
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/preact": "^2.0.1",
    "@testing-library/user-event": "^14.0.0",
    "chokidar": "^3.5.2",
    "esbuild": "^0.14.51",
    "htm": "^3.0.4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.12",
    "esbuild-serve": "^1.0.1"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "exports": {
    ".": "./public/daypickr.js",
    "./esm": "./public/esm/index.js",
    "./esm/index.js": "./public/esm/index.js",
    "./esm/*": "./public/esm/*"
  }
}
